# Firebase Configuration
# Copy this file to .env.dev and .env.prod and fill in your actual values
# DO NOT commit .env files to version control!

# Firebase Web Configuration
FIREBASE_API_KEY=your_api_key_here
FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID=your_app_id
FIREBASE_MEASUREMENT_ID=your_measurement_id

# Firebase Android Configuration  
FIREBASE_ANDROID_API_KEY=your_android_api_key
FIREBASE_ANDROID_APP_ID=your_android_app_id

# Firebase iOS Configuration
FIREBASE_IOS_API_KEY=your_ios_api_key
FIREBASE_IOS_APP_ID=your_ios_app_id
FIREBASE_IOS_BUNDLE_ID=com.zeekysocial.app

# Firebase Windows Configuration (if needed)
FIREBASE_WINDOWS_API_KEY=your_windows_api_key
FIREBASE_WINDOWS_APP_ID=your_windows_app_id

# Firebase macOS Configuration (if needed)
FIREBASE_MACOS_API_KEY=your_macos_api_key
FIREBASE_MACOS_APP_ID=your_macos_app_id
FIREBASE_MACOS_BUNDLE_ID=com.zeekysocial.app

# App Check (reCAPTCHA)
RECAPTCHA_V3_SITE_KEY=your_recaptcha_site_key

# Environment
FLUTTER_ENV=development

# Firebase Emulator Ports (for development)
FIRESTORE_EMULATOR_PORT=8080
AUTH_EMULATOR_PORT=9099
FUNCTIONS_EMULATOR_PORT=5001
STORAGE_EMULATOR_PORT=9199