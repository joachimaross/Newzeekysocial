{
  "name": "zeeky-social",
  "version": "1.0.0",
  "description": "A hybrid social media and messaging platform powered by AI",
  "private": true,
  "scripts": {
    "build": "flutter build web --release --web-renderer html",
    "build:canvaskit": "flutter build web --release --web-renderer canvaskit",
    "build:dev": "flutter build web --debug --web-renderer html --source-maps",
    "build:profile": "flutter build web --profile --web-renderer html",
    "dev": "flutter run -d web-server --web-port=3000",
    "dev:netlify": "netlify dev --config netlify.dev.toml",
    "serve": "flutter run -d web-server --web-port=8080 --release",
    "deploy:dev": "./scripts/deploy-dev.sh",
    "deploy:preview": "./scripts/deploy-dev.sh preview",
    "deploy:prod": "./scripts/deploy-dev.sh production",
    "analyze": "flutter analyze",
    "test": "flutter test",
    "clean": "flutter clean"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joachimaross/Newzeekysocial.git"
  },
  "author": "Joa'Chima Ross",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "flutter",
    "social-media",
    "messaging",
    "ai",
    "firebase",
    "web-app"
  ]
}